<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>P-SEEK</title>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <% include ./head/headIndex.ejs %>
</head>

<body id="page-top">
<!-- Navigation -->
<nav class="navbar navbar-expand-lg navbar-dark fixed-top" id="mainNav">
    <div class="container">
        <a class="navbar-brand js-scroll-trigger" href="#page-top">P-Seek :: <small>artists' playground</small></a>
        <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
            Menu
            <i class="fas fa-bars"></i>
        </button>
        <div class="collapse navbar-collapse" id="navbarResponsive">
            <ul class="navbar-nav text-uppercase ml-auto">
                <span hidden><%= navItem = ['services', 'product', 'artist', 'team', 'contact'] %></span>
                <% for (var i in navItem) { %>
                    <li class="nav-item">
                        <a class="nav-link js-scroll-trigger" href="#<%= navItem[i] %>">
                            <%= navItem[i] %>
                        </a>
                    </li>
                <% } %>
                <li class="nav-item" style="margin-top: 10px;">
                    <span id="user" style="color: #fed136; padding-right: 10px"></span>
                    <a class="btn btn-primary btn-sm" id="loginBtn" style="color: #474747" data-toggle="modal" href="#loginModal">Sign In</a>
                </li>
            </ul>
        </div>
    </div>
</nav>

<!-- Header -->
<header class="masthead">
    <div class="container">
        <div class="intro-text">
            <div class="intro-lead-in">Welcome to p-seek gallery</div>
            <div class="intro-heading text-uppercase">Everyone is Artist!</div>
            <a class="btn btn-primary btn-xl text-uppercase portfolio-link" data-toggle="modal" href="#portfolioModal7">PSEEK-GUIDE</a>
        </div>
    </div>
</header>

<!-- Services -->
<section id="services">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2 class="section-heading text-uppercase">Services</h2>
                <h3 class="section-subheading text-muted">There's three things that are p-seek offer to users.</h3>
            </div>
        </div>
        <div class="row text-center">
            <span hidden>
                <%= serviceItem = ['E-Ticket', 'Exhibit Artwork', 'My Art Museum'] %>
                <%= serviceDes = ['서비스 이용권 구매', '작품 감상', '나만의 아트 뮤지엄'] %>
                <%= serviceIcon = ['ticket-alt', 'landmark', 'users-cog'] %>
                <%= serviceLink = ['/', '/', '/myPage'] %>
            </span>
            <% for(let i in serviceItem) { %>
            <div class="col-md-4">
                <a href="<%= serviceLink[i] %>">
              <span class="fa-stack fa-4x">
                <i class="fas fa-circle fa-stack-2x text-primary"></i>
                <i class="fas fa-<%= serviceIcon[i] %> fa-stack-1x fa-inverse"></i>
              </span>
                </a>
                <h4 class="service-heading"><%= serviceItem[i] %></h4>
                <p class="text-muted"><%= serviceDes[i] %></p>
            </div>
            <% } %>
        </div>
    </div>
</section>

<!-- Portfolio Grid -->
<section class="bg-light" id="product">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2 class="section-heading text-uppercase">Product</h2>
                <h3 class="section-subheading text-muted">You can pick the art painting style that you want.</h3>
            </div>
        </div>
        <div class="row">
            <span hidden>
                <%= artGenre = ['contemporary', 'modern', 'pop', 'abstract', 'surrealism', 'stillLife'] %>
                <%= artName = ['현대미술', '모던아트', '팝아트', '추상화', '초현실주의', '정물화'] %>
            </span>
            <% for (let i in artGenre) { %>
            <div class="col-md-4 col-sm-6 portfolio-item">
                <a class="portfolio-link" href="/<%= artGenre[i] %>">
                    <div class="portfolio-hover">
                        <div class="portfolio-hover-content">
                            <i class="fas fa-plus fa-3x"></i>
                        </div>
                    </div>
                    <img class="img-fluid" src="/img/portfolio/0<%= ++i %>-thumbnail.jpg" alt="">
                </a>
                <div class="portfolio-caption">
                    <h4><%= artGenre[i-1] %> art</h4>
                    <p class="text-muted"><%= artName[i-1] %></p>
                </div>
            </div>
            <% } %>
        </div>
    </div>
</section>

<!-- Artist -->
<section id="artist">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2 class="section-heading text-uppercase">Artist</h2>
                <h3 class="section-subheading text-muted">Today's Best Artists who selected by p-seek.</h3>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <ul class="timeline">
                    <% artistNames.forEach((element, i) => { %>
                    <!-- index가 홀수이면 오른쪽 정렬 -->
                    <li<% if(i%2===1) { %> class="timeline-inverted" <% } else { %> class="" <% } %>>
                        <div class="timeline-image hovereffect">
                            <img class="rounded-circle img-fluid" src="img/about/<%= i+1 %>.jpg" alt="">
                            <img class="img-responsive" src="img/about/<%= i+1 %>.jpg">
                            <a href="#"><div class="overlay"></div></a>
                        </div>
                        <div class="timeline-panel">
                            <div class="timeline-heading">
                                <h4>:: <%= element["userId"] %></h4>
                                <h4 class="subheading"><%= element["userName"] %></h4>
                            </div>
                            <div class="timeline-body">
                                <p class="text-muted">Lorem ipsum dolor sit amet, consectetur adipisicing elit. Sunt ut voluptatum eius sapiente, totam reiciendis temporibus qui quibusdam, recusandae sit vero unde, sed, incidunt et ea quo dolore laudantium consectetur!</p>
                            </div>
                        </div>
                    </li>
                    <% }); %>
                    <li class="timeline-inverted">
                        <a href="#">
                            <div class="timeline-image">
                                <h4>Go to
                                    <br>Show more
                                    <br>Artists!</h4>
                            </div>
                        </a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</section>

<!-- Team -->
<section class="bg-light" id="team">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2 class="section-heading text-uppercase">Team PISA ::</h2>
                <h3 class="section-subheading text-muted">2019 Gachon-univ. graduate project team.</h3>
            </div>
        </div>
        <div class="row">
            <span hidden>
                <%= teamPISA = ['hyunji Yoo', 'hyunseoung Wi', 'byungki Park'] %>
                <%= teamRole = ['Developer & Designer', 'Developer & Writer', 'Developer & Troubleshooter'] %>
                <%= teamSns = ['twitter', 'facebook-f', 'linkedin-in'] %>
            </span>
            <% for(let i in teamPISA) { %>
            <div class="col-sm-4">
                <div class="team-member">
                    <img class="mx-auto rounded-circle" src="/img/team/<%= ++i %>.jpg" alt="">
                    <h4><%= teamPISA[i-1]%></h4>
                    <p class="text-muted"><%= teamRole[i-1] %></p>
                    <ul class="list-inline social-buttons">
                        <% for(let i in teamSns) { %>
                        <li class="list-inline-item">
                            <a href="#">
                                <i class="fab fa-<%= teamSns[i] %>"></i>
                            </a>
                        </li>
                        <% }%>
                    </ul>
                </div>
            </div>
            <% } %>
        </div>
        <div class="row">
            <div class="col-lg-8 mx-auto text-center">
                <p class="large text-muted">Lorem ipsum dolor sit amet, consectetur adipisicing elit. Aut eaque, laboriosam veritatis, quos non quis ad perspiciatis, totam corporis ea, alias ut unde.</p>
            </div>
        </div>
    </div>
</section>

<!-- Clients -->
<section class="py-5">
    <div class="container">
        <div class="row">
            <span hidden><%= clientItem = ['envato', 'designmodo', 'themeforest', 'creative-market'] %></span>
            <% for(let i in clientItem) { %>
            <div class="col-md-3 col-sm-6">
                <a href="#">
                    <img class="img-fluid d-block mx-auto" src="/img/logos/<%= clientItem[i] %>.jpg" alt="">
                </a>
            </div>
            <% } %>
        </div>
    </div>
</section>

<!-- Contact -->
<section id="contact">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2 class="section-heading text-uppercase">Contact Us</h2>
                <h3 class="section-subheading text-muted">Lorem ipsum dolor sit amet consectetur.</h3>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <form id="contactForm" name="sentMessage" novalidate="novalidate">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group">
                                <input class="form-control" id="name" type="text" placeholder="Your Name *" required="required" data-validation-required-message="Please enter your name.">
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="form-group">
                                <input class="form-control" id="email" type="email" placeholder="Your Email *" required="required" data-validation-required-message="Please enter your email address.">
                                <p class="help-block text-danger"></p>
                            </div>
                            <div class="form-group">
                                <input class="form-control" id="phone" type="tel" placeholder="Your Phone *" required="required" data-validation-required-message="Please enter your phone number.">
                                <p class="help-block text-danger"></p>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <textarea class="form-control" id="message" placeholder="Your Message *" required="required" data-validation-required-message="Please enter a message."></textarea>
                                <p class="help-block text-danger"></p>
                            </div>
                        </div>
                        <div class="clearfix"></div>
                        <div class="col-lg-12 text-center">
                            <div id="success"></div>
                            <button id="sendMessageButton" class="btn btn-primary btn-xl text-uppercase" type="submit">Send Message</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</section>

<!-- Footer -->
<footer>
    <div class="container">
        <div class="row">
            <div class="col-md-4">
                <span class="copyright">Copyright &copy; P-SEEK.com 2019</span>
            </div>
            <span hidden><%= iconItem = ['twitter', 'facebook-f', 'linkedin-in']%></span>
            <div class="col-md-4">
                <ul class="list-inline social-buttons">
                    <% for(let i in iconItem) { %>
                    <li class="list-inline-item">
                        <a href="#">
                            <i class="fab fa-<%= iconItem[i] %>"></i>
                        </a>
                    </li>
                    <% } %>
                </ul>
            </div>
            <div class="col-md-4">
                <ul class="list-inline quicklinks">
                    <li class="list-inline-item">
                        <a href="#">Privacy Policy</a>
                    </li>
                    <li class="list-inline-item">
                        <a href="#">Terms of Use</a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</footer>

<!-- Product Modals -->

<!-- login Modal -->
<div class="modal fade" id="loginModal" role="dialog">
    <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
            <div class="login-header" style="padding:35px 50px;">
                <!--<button type="button" class="loginClose" data-dismiss="modal">&times;</button>-->
                <h4 class="h4" align="center"><span style="padding-right: 10px"><i class="fas fa-home"></i></span>Login</h4>
            </div>
            <div class="modal-body" style="padding:40px 50px;">
                <form role="form">
                    <div class="form-group">
                        <label for="usrname">Username</label>
                        <input type="text" class="form-control" id="usrname" placeholder="Enter ID">
                    </div>
                    <div class="form-group">
                        <label for="psw">Password</label>
                        <input type="text" class="form-control" id="psw" placeholder="Enter password">
                    </div>
                    <div class="checkbox">
                        <label><input type="checkbox" value="" checked>Remember me</label>
                    </div>
                    <button type="submit" class="btn btn-warning btn-block">Login</button>
                </form>
            </div>
            <div class="modal-footer" id="login-footer">
                <button type="submit" class="btn btn-danger pull-left" onclick="">Sign Up</button>
                <button type="submit" class="btn btn-dark pull-left" onclick="">Find password</button>
            </div>
        </div>

    </div>
</div>
<script>
    $(document).ready(function(){
        $("#loginBtn").click(function(){
            $("#loginModal").modal();
        });
    });
</script>

